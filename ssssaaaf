import http.server
import socketserver

# Port, auf dem der Server lauschen wird
port = 8080

# Klasse für die Anfragebehandlung
class MeinRequestHandler(http.server.SimpleHTTPRequestHandler):
    def do_GET(self):
        if self.path == '/videos':
